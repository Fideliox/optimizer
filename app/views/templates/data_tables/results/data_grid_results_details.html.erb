<script type="text/html" id="data-grid-results-details" xmlns="http://www.w3.org/1999/html">
  <table cellpadding="0" cellspacing="0" border="0"
         width="100%" class="table table-striped table-bordered dataTable">
    <thead>
        <tr>
          <th>&nbsp;</th>
          <th>Type</th>
          <th>Cost Leg/Total</th>
          <th>TEU Avail/Accep</th>
          <th>Rate TEU</th>
          <th>Opp Value</th>
        </tr>
    </thead>
    <tbody>
        {{#list}}
        <tr>
          <td width="5px" class="text-center"><div class="more-dda manito" data-object="{{dda}}">+</div></td>
          <td width="10px" class="text-center">{{type}}</td>
          <td width="15px" class="text-center}">{{cost_per_teu_leg}}<strong>/</strong>{{total_cost_per_teu_leg}}</td>
          <td width="15px" class="text-center">{{max_units}}<strong>/</strong>{{sol_units_teu}}</td>
          <td width="15px" class="text-center">{{rate_per_teu}}</td>
          <td width="15px" class="text-center">{{opp_value_per_teu}}</td>
        </tr>
        <tr style="background-color:darkgrey">
          <td colspan="3" id="{{object}}" style="display:none"></td>
        </tr>
        {{/list}}
    </tbody>
</table>
</script>